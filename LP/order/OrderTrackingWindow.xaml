<Window x:Class="PL.OrderTrackingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="OrderTrackingWindow" Height="450" Width="800">
    <Grid>
        <TextBox Style="{StaticResource classicText}" Margin="123,71,552,336" TextWrapping="Wrap" Text="ID Order" Width="Auto" Height="27"/>
        <TextBox x:Name="IdOrderText" Style="{StaticResource classicText}" Margin="283,71,439,336" TextWrapping="Wrap" Text="0000" Width="78" Height="27"/>
        <TextBox Style="{StaticResource classicText}" HorizontalAlignment="Left" Margin="123,115,0,0" TextWrapping="Wrap" Text="Customer Name" VerticalAlignment="Top" Width="125" Height="27"/>
        <TextBox x:Name="NameCustomerText" Style="{StaticResource classicText}" HorizontalAlignment="Left" Margin="283,115,0,0" TextWrapping="Wrap" Text="AAAA" VerticalAlignment="Top" Width="78" Height="27"/>

    </Grid>
</Window>
